#include <stdio.h>
int main()
{
    int i, j, t1 = 0, t2 = 1, n= 0;

    printf("Enter the number of terms:\n ");
    scanf("%d",&j);

    printf(" %d, %d, ", t1, t2);

    for (i=3; i <= j; ++i)
    {
        n = t1 + t2;
        t1 = t2;
        t2 = n;
        printf("%d, ",n);
    }
    return 0;
}
