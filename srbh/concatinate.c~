#include<stdio.h>
#include<string.h>
int main()		
{ 

char str1[100],str2[100],str3[100];

printf("\nENTER STRING 1\n");
gets(str1);

printf("\nENTER STRING 2\n");
gets(str2);

printf("\nENTER STRING 3\n");
gets(str3);

strcat(str1,str2);
strcat(str1,str3);

printf("\nCONCATINATED STRING:%s\n",str1);
 

return 0;

}
