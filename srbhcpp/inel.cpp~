#include<iostream>
using namespace std;
int main()
{
int a[50],i,n,pos,value;
cout<<("Enter the size of array\n");
cin>>n;

cout<<("Enter the elements\n");

for(i=0;i<n;i++)

cin>>a[i];
cout<<("Enter the location to insert the element\n");
cin>>pos;
cout<<("Enter the value to insert\n");
cin>>value;

for(i=n;i>=pos;i--)

a[i]=a[i-1];
a[pos-1]=value;
cout<<("Array after insertation of the element\n");

for(i=0;i<=n;i++)

cout<<a[i]<<"\n";
return 0;
}

